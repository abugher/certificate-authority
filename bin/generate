#!/bin/bash

# Find self.
OLDPWD="${PWD}"
cd "${0%/*}/.."
certificate_authority="${PWD}"
cd "${OLDPWD}"

# Load libraries.
libs=(
  'generate_sanity'
  'generate_ca_root'
  'generate_ca_intermediate'
  'generate_host'
)
if ! . "${certificate_authority}/lib/common.sh"; then
  echo "Failed to load common dependencies." >&2
  exit 1
fi

generate_sanity
generate_ca_root
generate_ca_intermediate
generate_host
